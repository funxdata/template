{
    "name": "@funxdata/template",
    "version": "0.1.1",
    "license": "MIT",
    "exports": {
      "./comps":"./comps/index.ts"
    },
    "nodeModulesDir": "auto",
    "exclude": [
        ".github",
        "node_modules",
        "index.html",
        ".prettierrc",
        "plugin",
        "src"
    ],
    "tasks": {
        "view": "deno run --allow-sys --allow-net --allow-read  --allow-run --allow-env --allow-ffi jsr:@funxdata/toolkit/view",
        "pack":"deno run --allow-net --allow-read --allow-write --allow-env --allow-ffi --allow-run jsr:@funxdata/toolkit/pack",
        "release":"deno run --allow-net --allow-read  --allow-run --allow-ffi --allow-env --allow-sys jsr:@funxdata/toolkit/release"
    },
    "imports": {
        "@/": "./",
        "@funxdata/toolkit": "jsr:@funxdata/toolkit@^0.1.30",
        "@std/assert": "jsr:@std/assert@1",
        "@std/path": "jsr:@std/path@^1.1.1"
    },
    "compilerOptions": {
        "lib": ["dom", "esnext","deno.ns", "deno.unstable"]
    }
}
